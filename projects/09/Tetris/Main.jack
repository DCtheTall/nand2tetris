/** Main class initializes, runs, and finally disposes the event loop. */
class Main {
    function void main() {
        var GameLoop loop;
        let loop = GameLoop.new();
        
        // TODO prompt for seed
        do Random.setSeed(12345);
        do Renderer.init();
        do Brick.init();

        do loop.run();
        do loop.dispose();
        return;
    }
}
